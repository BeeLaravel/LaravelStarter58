{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "appstract/laravel-options": "^2.2",
        "artisaninweb/laravel-soap": "0.3.0.9",
        "awssat/laravel-visits": "^1.5",
        "barryvdh/laravel-cors": "^0.11.3",
        "barryvdh/laravel-snappy": "^0.4.4",
        "beesoft/weather": "dev-master",
        "binarytorch/larecipe": "^2.0",
        "chumper/zipper": "^1.0",
        "cmgmyr/messenger": "^2.17",
        "dingo/api": "^2.2",
        "fideloper/proxy": "^4.0",
        "graham-campbell/markdown": "^10.3",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "laravolt/avatar": "^2.2",
        "lorisleiva/laravel-search-string": "^0.1.3",
        "lubusin/laravel-decomposer": "^1.2",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^1.3",
        "phenx/php-font-lib": "^0.5.1",
        "qoraiche/laravel-mail-editor": "^1.1",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "smalot/pdfparser": "^0.14.0",
        "spatie/laravel-flash": "^1.2",
        "spatie/laravel-schemaless-attributes": "^1.4",
        "spatie/valuestore": "^1.2",
        "start-point/etcd-php": "^1.1",
        "swoole/etcd-client": "^1.0",
        "vinkla/hashids": "^6.0"
    },
    "require-dev": {
        "barryvdh/laravel-translation-manager": "^0.5.3",
        "beyondcode/laravel-dump-server": "^1.0",
        "beyondcode/laravel-er-diagram-generator": "^1.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "itsgoingd/clockwork": "^3.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "orangehill/iseed": "^2.6",
        "overtrue/package-builder": "^1.0",
        "phpunit/phpunit": "^7.5",
        "rap2hpoutre/laravel-log-viewer": "^1.1",
        "viacreative/sudo-su": "^1.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "weather": {
            "type": "path",
            "url": "../weather"
        },
        "packagist": {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        }
    }
}
